  
<div class="row">
  <div class="row">
    <form id="searchForm" class="container-fluid">
      <input id="zipcodeInput"  type="text" placeholder="zipcode" /> 
      <input id="eventDateInput" type="text" placeholder="mm/dd/yyyy" />
      <button id="searchEventButton" type="submit" >Search</button>
    </form>
  </div>
  <div class="row">
    <div id="astroEventList" class="list-group overflow-auto col-sm-7 col-md-7 col-lg-7">
      <ul class="list-group">
            {{#each astroEvents as |astroEvent|}}
        <li class="list-group-item d-flex justify-content-between align-items-center">
        {{astroEvent.id}}
        {{astroEvent.description}}
        <span class="float-right">
          <a id="saveButton"><i class="fa-solid fa-floppy-disk"></i></a>
          <a id="saveButton"><i class="fa-solid fa-calendar-plus"></i></a>
        </span>
        </li>
    {{/each}}
    {{#each astroEvents as |astroEvent|}}
        <li class="list-group-item d-flex justify-content-between align-items-center">
        {{astroEvent.id}}
        {{astroEvent.description}}
        <span class="float-right">
          <a id="saveButton"><i class="fa-solid fa-floppy-disk"></i></a>
          <a id="saveButton"><i class="fa-solid fa-calendar-plus"></i></a>
        </span>
        </li>
    {{/each}}
    </ul>
    </div>
    <div class="col-sm-5 col-md-5 col-lg-5">
        <p><img class="img-thumbnail" src="https://apod.nasa.gov/apod/image/2305/shackleton_arrow.png" alt="space image"></p>
        <p>description</p>
    </div>
  </div>
</div>


